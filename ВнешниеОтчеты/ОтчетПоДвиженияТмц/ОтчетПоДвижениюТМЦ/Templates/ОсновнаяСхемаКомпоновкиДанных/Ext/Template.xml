;

////////////////////////////////////////////////////////////////////////////////
УНИЧТОЖИТЬ ВТПоследнееПоступление
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТОстатки.Счет КАК Счет,
	ВТОстатки.Номенклатура КАК Номенклатура,
	ВТОстатки.Склад КАК Склад,
	ВТОстатки.КоличествоНачальныйОстаток КАК КоличествоНачальныйОстаток,
	ВТОстатки.СуммаНачальныйОстаток КАК СуммаНачальныйОстаток,
	ВТОстатки.КоличествоКонечныйОстаток КАК КоличествоКонечныйОстаток,
	ВТОстатки.СуммаКонечныйОстаток КАК СуммаКонечныйОстаток,
	ЕСТЬNULL(ВТПоследниеПоступленияСКоличествомСуммой.ДатаПоступления, ДАТАВРЕМЯ(1, 1, 1)) КАК ДатаПоступления,
	ЕСТЬNULL(ВТПоследниеПоступленияСКоличествомСуммой.КоличествоПоступление, 0) КАК КоличествоПоступления,
	ЕСТЬNULL(ВТПоследниеПоступленияСКоличествомСуммой.СуммаПоступление, 0) КАК СуммаПоступления,
	ВЫБОР
		КОГДА ВТПоследниеПоступленияСКоличествомСуммой.ДатаПоступления ЕСТЬ NULL
			ТОГДА 0
		ИНАЧЕ РАЗНОСТЬДАТ(ЕСТЬNULL(ВТПоследниеПоступленияСКоличествомСуммой.ДатаПоступления, ДАТАВРЕМЯ(1, 1, 1)), &amp;КонецПериода, ДЕНЬ) + 1
	КОНЕЦ КАК ДнейБезДвиженияНаСкладе,
	ВЫБОР
		КОГДА ВТОстатки.КоличествоКонечныйОстаток &lt;= ЕСТЬNULL(ВТПоследниеПоступленияСКоличествомСуммой.КоличествоПоступление, 0)
			ТОГДА ВТОстатки.КоличествоКонечныйОстаток
		ИНАЧЕ ЕСТЬNULL(ВТПоследниеПоступленияСКоличествомСуммой.КоличествоПоступление, 0)
	КОНЕЦ КАК КоличествоБезДвижения,
	ВЫБОР
		КОГДА ВТОстатки.СуммаКонечныйОстаток &lt;= ЕСТЬNULL(ВТПоследниеПоступленияСКоличествомСуммой.СуммаПоступление, 0)
			ТОГДА ВТОстатки.СуммаКонечныйОстаток
		ИНАЧЕ ЕСТЬNULL(ВТПоследниеПоступленияСКоличествомСуммой.СуммаПоступление, 0)
	КОНЕЦ КАК СуммаБезДвижения
ИЗ
	ВТОстатки КАК ВТОстатки
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТПоследниеПоступленияСКоличествомСуммой КАК ВТПоследниеПоступленияСКоличествомСуммой
		ПО ВТОстатки.Счет = ВТПоследниеПоступленияСКоличествомСуммой.Счет
			И ВТОстатки.Номенклатура = ВТПоследниеПоступленияСКоличествомСуммой.Номенклатура
			И ВТОстатки.Склад = ВТПоследниеПоступленияСКоличествомСуммой.Склад
;

